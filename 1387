import java.util.Scanner;
import java.io.*;
public class Main {
    public static void main(String[] args) {
		PrintStream sa = System.out;
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt(),b = sc.nextInt();
		while(a!=0 && b!=0){
			sa.println(a+b);
			a = sc.nextInt();b = sc.nextInt();
		}
		
		     
   }
}
