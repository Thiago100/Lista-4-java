import java.util.Scanner;
import java.io.*;
public class Main {
    public static void main(String[] args) {
		PrintStream sa = System.out;
		Scanner sc = new Scanner(System.in);
		int soma=0,a = sc.nextInt(),i=0;
		while(a>=0){
			i++;
			soma+=a;
			a = sc.nextInt();
		}
		double x = (double)soma/i;
		sa.printf("%.2f\n",x);
		     
   }
}
