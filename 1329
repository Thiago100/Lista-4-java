import java.util.Scanner;
import java.io.*;
public class Main {
    public static void main(String[] args) {
		PrintStream sa = System.out;
		Scanner sc = new Scanner(System.in);
		int x=0,m=0,j=0,a = sc.nextInt(),i=0;
		while(a>0){
			for(i=0;i<a;i++){
				x = sc.nextInt();
				if(x==0) m++;
				else j++;
			}
			sa.println("Mary won "+m+" times and John won "+j+" times");
			m=0;j=0;i=0;
			a=sc.nextInt();
		}
		     
   }
}
