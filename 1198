import java.util.*;
import java.io.*;
// sc.hasNextInt() //EOF
public class Main{
	public static void main(String[] args){
        PrintStream sa = System.out;
        Scanner sc = new Scanner(System.in);
        while(sc.hasNext()){
            long a = sc.nextLong(),b = sc.nextLong(),x=0;
            if(a>b) x = a-b;
            if(a<b) x = b-a;
            sa.println(x);
            x=0;


        }


	}
}
