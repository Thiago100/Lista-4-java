import java.util.Scanner;
import java.io.*;
public class Main {
    public static void main(String[] args) {
		PrintStream sa = System.out;
		Scanner sc = new Scanner(System.in);
		int x=0;
		while(x!=2002){
			x = sc.nextInt();
			if(x!=2002){
				sa.println("Senha Invalida");
			}else{
				sa.println("Acesso Permitido");
			}
		}
		     
   }
}
