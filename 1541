import java.util.*;
import java.io.*;
public class Main{
	public static void main(String[] args){
        PrintStream sa = System.out;
        Scanner sc = new Scanner(System.in);
        int v=0,a = 3,b,c;
        double z=0,x=0;
        while(a!=0){
            a = sc.nextInt();if(a==0) break;
            b = sc.nextInt();c = sc.nextInt();
            x = a*b;
            x = x*100/c;
            z = Math.sqrt(x);
            v = (int)z;
            sa.printf("%d\n",v);

        }


	}
}
